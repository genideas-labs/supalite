{
  "name": "supalite",
  "version": "0.1.8",
  "description": "A lightweight TypeScript PostgreSQL client with Supabase-style API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "$npm_execpath run build",
    "prepublishOnly": "$npm_execpath test && $npm_execpath run lint",
    "dev": "ts-node src/index.ts"
  },
  "packageManager": "npm@10.2.4",
  "engines": {
    "node": ">=16.17.0"
  },
  "keywords": [
    "supabase",
    "postgresql",
    "typescript",
    "database",
    "lightweight",
    "query-builder"
  ],
  "author": {
    "name": "Wondong Shin",
    "email": "wodshin@gmail.com",
    "organization": "Genideas Inc."
  },
  "license": "MIT",
  "dependencies": {
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.4.7",
    "jwt-decode": "^4.0.0",
    "pg": "^8.11.3",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.0",
    "@types/websocket": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
